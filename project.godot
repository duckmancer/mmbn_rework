[application]

config/name="Megaman Battle Network"
run/main_scene="res://Overworld/Overworld.tscn"

[autoload]

SpriteAssets="*res://Globals/SpriteAssets.gd"
AudioAssets="*res://Globals/AudioAssets.gd"
Constants="*res://Globals/Constants.gd"
Utils="*res://Utilities/Utils.gd"
Globals="*res://Globals/Globals.gd"
Scenes="*res://Globals/Scenes.gd"
ActionData="*res://Globals/ActionData.gd"
Battlechips="*res://Globals/Battlechips.gd"
EncounterPool="*res://Globals/EncounterPool.gd"
PlayerData="*res://Globals/PlayerData.gd"
Transition="*res://Utilities/Transition.tscn"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/width=240
window/size/height=160
window/size/test_width=1200
window/size/test_height=800
window/stretch/mode="2d"

[global]

snap=false

